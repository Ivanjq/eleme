    url('../assets/fonts/sell-icon.ttf?2430tu') format('truetype'),
    url('../assets/fonts/sell-icon.woff?2430tu') format('woff'),
    url('../assets/fonts/sell-icon.svg?2430tu#sell-icon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
